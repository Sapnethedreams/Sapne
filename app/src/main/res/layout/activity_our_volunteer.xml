<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/logo_sapne"
    tools:context="com.example.intents.sapne.our_volunteer">





    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Volunteer Head"
                android:textSize="35sp"
                android:background="#05d5f5"
                android:layout_gravity="center"
                android:textColor="#f16101"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="29dp"
                android:layout_marginTop="7dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/button6"
                android:layout_alignStart="@+id/button6"
                android:layout_alignTop="@+id/imageView6"
                app:srcCompat="@drawable/vaibhav1" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/button5"
                android:layout_alignStart="@+id/button5"
                android:layout_below="@+id/textView5"
                android:layout_gravity="center"
                app:srcCompat="@drawable/nikita1"
                />
            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/button5"
                android:layout_alignStart="@+id/button5"
                android:layout_below="@+id/textView5"
                android:layout_gravity="center"
                app:srcCompat="@drawable/lakshita1"></ImageView>
            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/button6"
                android:layout_alignStart="@+id/button6"
                android:layout_alignTop="@+id/imageView6"
                app:srcCompat="@drawable/jasmine" />
            <ImageView
                android:id="@+id/imageView36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/button5"
                android:layout_alignStart="@+id/button5"
                android:layout_below="@+id/textView5"
                android:layout_gravity="center"
                app:srcCompat="@drawable/falguni1"></ImageView>
            <ImageView
                android:id="@+id/imageView56"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/button5"
                android:layout_alignStart="@+id/button5"
                android:layout_below="@+id/textView5"
                android:layout_gravity="center"
                app:srcCompat="@drawable/richa1"></ImageView>


            <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bhumika1"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/button5"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/indraja1"
        android:layout_alignTop="@+id/imageView"
        android:layout_alignLeft="@+id/button8"
        android:layout_alignStart="@+id/button8"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp" />
            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/rawat"
                android:layout_alignTop="@+id/imageView"
                android:layout_alignLeft="@+id/button8"
                android:layout_alignStart="@+id/button8"
                android:layout_marginLeft="11dp"
                android:layout_marginStart="11dp" />
            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ishika"
                android:layout_alignTop="@+id/imageView"
                android:layout_alignLeft="@+id/button8"
                android:layout_alignStart="@+id/button8"
                android:layout_marginLeft="11dp"
                android:layout_marginStart="11dp" />
            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/kiranjeet"
                android:layout_alignTop="@+id/imageView"
                android:layout_alignLeft="@+id/button8"
                android:layout_alignStart="@+id/button8"
                android:layout_marginLeft="11dp"
                android:layout_marginStart="11dp" />






            <Button
        android:id="@+id/button9"
        android:onClick="vol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="view ALL Volunteers"
                android:layout_gravity="center"
        android:textColor="#222222"
        android:background="#fc2f81"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
        </LinearLayout>
    </ScrollView>

</RelativeLayout>
