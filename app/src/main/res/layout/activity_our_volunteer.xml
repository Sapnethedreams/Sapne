<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:background="#05d5f5"
                android:text="@string/volunteer_head"
                android:textColor="#f16101"
                android:textSize="30sp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="29dp" />

            />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/imageView26"
                    android:layout_width="191dp"
                    android:layout_height="200dp"
                    android:layout_gravity="left"
                    app:srcCompat="@drawable/falguni" />

                <TextView
                    android:id="@+id/textView45"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="#ffffff"
                    android:text="@string/falguni_jain_coordinator"
                    android:textColor="#f16101"
                    android:textSize="35sp"
                    tools:layout_editor_absoluteX="72dp"
                    tools:layout_editor_absoluteY="29dp" />
            </LinearLayout>

            />



            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView125"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="#ffffff"
                    android:text="@string/gurleen_camp_coordinator"
                    android:textColor="#f16101"
                    android:textSize="28sp"
                    tools:layout_editor_absoluteX="72dp"
                    tools:layout_editor_absoluteY="29dp" />
                <ImageView
                    android:id="@+id/imageView36"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_gravity="right"
                    app:srcCompat="@drawable/gurleen"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/imageView24"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_gravity="left"
                    app:srcCompat="@drawable/piyush" />
                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="#ffffff"
                    android:text="@string/piyush_shrivastava_app_development_head"
                    android:textColor="#f16101"
                    android:textSize="25sp"
                    tools:layout_editor_absoluteX="72dp"
                    tools:layout_editor_absoluteY="29dp" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="#ffffff"
                    android:text="Nishant Parkey Web Development Head"
                    android:textColor="#f16101"
                    android:textSize="25sp"
                    tools:layout_editor_absoluteX="72dp"
                    tools:layout_editor_absoluteY="29dp" />
                <ImageView
                    android:id="@+id/imageView56"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_gravity="right"
                    app:srcCompat="@drawable/nishant"/>
            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/imageView124"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_gravity="left"
                    app:srcCompat="@drawable/harleen" />
                <TextView
                    android:id="@+id/textView155"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="#ffffff"
                    android:text="@string/harleen_kaur_graphic_department_head"
                    android:textColor="#f16101"
                    android:textSize="25sp"
                    tools:layout_editor_absoluteX="72dp"
                    tools:layout_editor_absoluteY="29dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView295"
                    android:layout_width="198dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="#ffffff"
                    android:text="@string/pallavi_madan_hr_head"
                    android:textColor="#f16101"
                    android:textSize="25sp"
                    tools:layout_editor_absoluteX="72dp"
                    tools:layout_editor_absoluteY="29dp" />

                <ImageView
                    android:id="@+id/imageView556"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_gravity="right"
                    app:srcCompat="@drawable/pallavi"
                    android:contentDescription="@string/pallavi_madan_hr_head"/>
            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/imageView004"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_gravity="left"
                    app:srcCompat="@drawable/gurleen_kaur" />
                <TextView
                    android:id="@+id/textView005"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="#ffffff"
                    android:text="@string/gurleen_kaur_social_media_head"
                    android:textColor="#f16101"
                    android:textSize="25sp"
                    tools:layout_editor_absoluteX="72dp"
                    tools:layout_editor_absoluteY="29dp" />
            </LinearLayout>







            <Button
                android:id="@+id/button9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#fc2f81"
                android:onClick="vol"
                android:text="@string/view_all_volunteers"
                android:textColor="#222222" />
        </LinearLayout>

    </ScrollView>


</RelativeLayout>
